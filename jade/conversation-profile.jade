    .poppi-chat
      .header.header-conversation-profile
        .infor-conversation-profile
          a(href='#')
            img.media-object(src="images/user.jpg")
          .user-info
            h4.media-heading luuminhtan
            span luuminhtan123@gmail.com
        .info-account-sign
          i.icon-sign-out
          span Sign out
                
      .sidebar-left
        ul.menu-left
