                img.media-object(src="images/user.jpg")
                input(type="text", placeholder='Type a name and hit Enter')
              .hr
                span Detail
              .info-profile-tabs-user
                label Location:
                span Ho Chi Minh city, Vietnam
                label Email:
                span guest.12022016@gmail.com
                label First seen:
                span 19 days ago
                label Last seen:
                span 6 days ago
                label Browser language:
                span Vietnamese
                label Browser:
                span Chrome
                label Browser version:
                span 49.0.2623.91
                label Web sessions:
                span 23
              .hr
                span Action
              .action-profile-tabs-user
                a(href="#",class="btn-profile delete")
                  span Delete person
                  i Delete this person and their conversation history
                a(href="#",class="btn-profile")
                  span Block person
                  i Block this person and future messages from them
                a(href="#",class="btn-profile")
                  span Unsubscribe from email
                  i Stop sending this person email
          #tabs-password
            .content-tabs-password
              .hr
                span Change your password
              #changePassword
                .group-form-custom
                  label Current password
                  input(type='password' class='txt-custom')
                .group-form-custom
                  label New password
                  input(type='password' class='txt-custom')
                .group-form-custom
                  label Confirm new password
                  input(type='password' class='txt-custom')
              .actionUpdatePass
                a(href="#", class='btnp btnp-default') Update
                a(href="#", class='btnp btnp-cancel') Cancel

          #tabs-profile-user
            .content-tabs-profile
              #update-avatar.avatar-form
                img(src="images/user.jpg")
                .form-avatar
                  label Your profile picture
                  a(href="#", class='btnp btnp-none btnp-lg') Update
              .hr
                span Profile
              #profile-Form
                .group-form-custom
                  label Name
                  input(type='text' class='txt-custom',placeholder='LuuminhTan')
                .group-form-custom
                  label Email
                  input(type='text' class='txt-custom',placeholder='luuminhtan@gmail.com')
              .actionUpdateProfile
