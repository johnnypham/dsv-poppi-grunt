.content-left
  display inline-block
  float left
  width 480px
  background-color #043b5a
  overflow scroll
  height 100%

.ui-tabs-nav
  li
    display inline-block
    width 50%
    float left
    background-color #0a4a6f
    text-align center
    padding 17px 0px
    border-bottom 3px solid #0a4a6f
    a
      color #fff
      font-size 14px


  .ui-tabs-active
    border-color #fff