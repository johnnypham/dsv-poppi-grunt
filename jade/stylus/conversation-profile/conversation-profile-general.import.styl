#tabs-conversation-profile
  height 100%
  background-color #ebebeb
  .ui-tabs-nav
    height 80px
    position fixed
    top 80px
    left 85px
    display inline-block
    width calc(100% - 30px)
    background-color #1074ae
    padding-left 30px
    li
      display inline-block
      float left
      height 78px
      &.ui-tabs-active
        border-bottom 2px solid #fff
      a
        line-height 120px
        color #ffffff
        padding 0px 45px